{
  "name": "everhood-platform",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate --schema=src/prisma/schema.prisma",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "cron:news": "ts-node scripts/fetchNews.ts",
    "cron:genz": "ts-node scripts/fetchGenZContent.ts",
    "cron:jobs": "ts-node scripts/fetchJobs.ts",
    "cron:daily-digest": "ts-node lib/tasks/generateDailyDigest.ts",
    "cron:all": "npm run cron:news && npm run cron:genz && npm run cron:jobs && npm run cron:daily-digest",
    "cron:ai-cache": "ts-node lib/tasks/cacheAIGeneration.ts",
    "db:seed": "prisma db seed",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "typecheck": "tsc --noEmit",
    "prisma:validate": "prisma validate",
    "check": "npm run lint && npm run typecheck && npm run prisma:validate",
    "fix:use-client-directive": "node fix-use-client-directive.js",
    "fix:import-paths": "node fix-import-paths.js",
    "fix:missing-exports": "node fix-missing-exports.js",
    "fix:mui-required-props": "node fix-mui-required-props.js",
    "fix:mui-grid-to-box": "node fix-mui-grid-to-box.js",
    "fix:implicit-any": "node fix-implicit-any.js",
    "fix:all": "npm run fix:use-client-directive && npm run fix:import-paths && npm run fix:missing-exports && npm run fix:mui-required-props && npm run fix:mui-grid-to-box && npm run fix:implicit-any",
    "fix:lint": "node fix-lint-errors.js",
    "redis:start": "brew services start redis",
    "redis:stop": "brew services stop redis",
    "postgres:start": "brew services start postgresql",
    "postgres:stop": "brew services stop postgresql",
    "setup:local": "npm run redis:start && npm run postgres:start && npm install && npx prisma migrate dev && npx prisma db seed"
  },
