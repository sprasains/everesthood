  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "cron:news": "ts-node scripts/fetchNews.ts",
    "cron:genz": "ts-node scripts/fetchGenZContent.ts",
    "cron:jobs": "ts-node scripts/fetchJobs.ts",
    "cron:daily-digest": "ts-node lib/tasks/generateDailyDigest.ts",
    "cron:all": "npm run cron:news && npm run cron:genz && npm run cron:jobs && npm run cron:daily-digest",
    "cron:ai-cache": "ts-node lib/tasks/cacheAIGeneration.ts",
    "db:seed": "prisma db seed",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "typecheck": "tsc --noEmit",
    "prisma:validate": "prisma validate",
    "check": "npm run lint && npm run typecheck && npm run prisma:validate",
    "fix:use-client-directive": "node fix-use-client-directive.js",
    "fix:import-paths": "node fix-import-paths.js",
    "fix:missing-exports": "node fix-missing-exports.js",
    "debug": "FEATURE_DEBUG=true npm run dev",
    "debug:logs": "tail -f logs/app.log | jq '.'",
    "debug:errors": "tail -f logs/app.log | jq '.level == \"error\"'",
    "debug:warnings": "tail -f logs/app.log | jq '.level == \"warn\"'",
    "debug:info": "tail -f logs/app.log | jq '.level == \"info\"'",
    "debug:db": "npx prisma studio",
    "debug:redis": "redis-cli monitor",
    "debug:queue": "open http://localhost:3001",
    "debug:health": "curl http://localhost:3000/api/debug/health | jq '.'",
    "debug:env": "curl http://localhost:3000/api/debug/env | jq '.'",
    "debug:performance": "curl http://localhost:3000/api/debug/performance | jq '.'",
    "debug:requests": "curl 'http://localhost:3000/api/debug/requests?limit=10' | jq '.'",
    "debug:logs-api": "curl 'http://localhost:3000/api/debug/logs?level=error&limit=20' | jq '.'",
    "debug:schema": "curl http://localhost:3000/api/debug/schema | jq '.'",
    "debug:cache": "curl http://localhost:3000/api/debug/cache | jq '.'",
    "debug:test": "curl -X POST http://localhost:3000/api/debug/test -H 'Content-Type: application/json' -d '{\"method\":\"GET\",\"url\":\"http://localhost:3000/api/debug/health\"}' | jq '.'",
    "debug:panel": "open http://localhost:3000/debug",
    "debug:all": "npm run debug:health && npm run debug:performance && npm run debug:env",
    "fix:mui-required-props": "node fix-mui-required-props.js",
    "fix:mui-grid-to-box": "node fix-mui-grid-to-box.js",
    "fix:implicit-any": "node fix-implicit-any.js",
    "fix:all": "npm run fix:use-client-directive && npm run fix:import-paths && npm run fix:missing-exports && npm run fix:mui-required-props && npm run fix:mui-grid-to-box && npm run fix:implicit-any",
    "fix:lint": "node fix-lint-errors.js",
    "redis:start": "brew services start redis",
    "redis:stop": "brew services stop redis",
    "postgres:start": "brew services start postgresql",
    "postgres:stop": "brew services stop postgresql",
    "setup:local": "npm run redis:start && npm run postgres:start && npm install && npx prisma migrate dev && npx prisma db seed",
    "fix:missing-env": "ts-node scripts/fix-missing-env.ts",
    "fix:imports": "ts-node scripts/fix-imports.ts",
    "fix:dead-code": "ts-node scripts/fix-dead-code.ts",
    "fix:routes": "ts-node scripts/fix-routes.ts",
    "check:routes": "ts-node scripts/find-duplicate-routes.ts",
    "report:similarity": "ts-node scripts/generate-similarity-report.ts",
    "propose:route-merges": "ts-node scripts/propose-route-merges.ts",
    "open:route-diff": "ts-node scripts/open-vscode-diff.ts"
